<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.system.mapper.NegativePressureWardMapper">

    <select id="getList" resultType="com.ruoyi.system.entity.NegativePressureWard">
        SELECT * from negative_pressure_ward WHERE access_time between #{beginTime} and #{endTime} and is_deleted = 0
    </select>

</mapper>
